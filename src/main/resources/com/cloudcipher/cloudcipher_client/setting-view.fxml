<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minWidth="500" prefWidth="500.0" stylesheets="@css/Common.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cloudcipher.cloudcipher_client.SettingController">
    <BorderPane layoutY="-1.0" prefWidth="500.0">
        <center>
            <GridPane hgap="8.0" vgap="16.0" BorderPane.alignment="CENTER">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="254.0" minWidth="10.0" prefWidth="208.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="341.0" minWidth="10.0" prefWidth="292.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <Button minWidth="120.0" mnemonicParsing="false" onAction="#handleChangeDirectoryButtonClick" prefWidth="200.0" text="Default Directory">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Label fx:id="selectedDirectoryLabel" textOverrun="CENTER_WORD_ELLIPSIS" GridPane.columnIndex="1" />
                <Button minWidth="120.0" mnemonicParsing="false" onAction="#handleConfigButtonClick" prefWidth="200.0" text="Application Config" GridPane.rowIndex="1">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Label fx:id="configLabel" textOverrun="CENTER_WORD_ELLIPSIS" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <padding>
               <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </padding>
            </GridPane>
        </center>
        <bottom>
            <HBox alignment="CENTER" prefHeight="80.0" prefWidth="500.0" BorderPane.alignment="CENTER">
                <Button minHeight="25.0" minWidth="100.0" mnemonicParsing="false" onAction="#handleCloseButtonClick" text="Close">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
            </HBox>
        </bottom>
    </BorderPane>
</AnchorPane>
